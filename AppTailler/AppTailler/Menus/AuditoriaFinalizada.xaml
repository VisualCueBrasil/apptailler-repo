<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 x:Class="AppTailler.AuditoriaFinalizada"
       >
  <ContentPage.Content>
    <AbsoluteLayout AbsoluteLayout.LayoutBounds="0.90,0.30" AbsoluteLayout.LayoutFlags="PositionProportional">
      <StackLayout BackgroundColor="#CDD1D7" Padding="20,10,20,10" Orientation="Vertical" Spacing="5" AbsoluteLayout.LayoutBounds="0.5,0.5" AbsoluteLayout.LayoutFlags="PositionProportional" HorizontalOptions="CenterAndExpand">
        <Label Text="AUDITORIA FINALIZADA COM SUCESSO!" XAlign="Center" Font="Bold,36" TextColor="#384D79"></Label>
        <AbsoluteLayout HorizontalOptions="CenterAndExpand">
          <Image Source="finalizado.png"></Image>
        </AbsoluteLayout>
      </StackLayout>
    </AbsoluteLayout>
  </ContentPage.Content>
</ContentPage>